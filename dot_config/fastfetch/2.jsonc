        // },
        // {
        //     "type": "sound",
        //     "key": "  "
        // },
        // {
        //     "type": "gamepad",
        //     "key": "  "
        // },
        {
            "type": "custom", // SoftwareStart
            "format": "├────────────────────── \u001b[1m󱃸 \u001b[0m ────────────────────┤"
        },
        {
            "type": "title",
            "key": "  ",
            "format": "{1}@{2}"
        },
        {
            "type": "os",
            "key": "  "
        },
        {
            "type": "kernel",
            "key": "  ",
            "format": "{1} {2}"
        },
        {
            "type": "lm",
            "key": "  󰧨"
        },
        {
            "type": "de",
            "key": "  "
        },
        {
            "type": "wm",
            "key": "  "
        },
        {
            "type": "shell",
            "key": "  "
        },
        {
            "type": "terminal",
            "key": "  "
        },
        {
            "type": "terminalfont",
            "key": "  "
        },
        {
            "type": "theme",
            "key": "  󰉼",
            "format": "{1}"
        },
        {
            "type": "icons",
            "key": "  󰀻"
        },
        // {
